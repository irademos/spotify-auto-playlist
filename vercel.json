  {
    "version": 2,
    "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/build",
        "outputDirectory": "client/build"
      }
    },
    {
      "src": "client/package.json",
      "use": "@vercel/node"
    }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/build/$1"
      }
    ],
    "buildCommand": "cd client && npm run build"
  }
  